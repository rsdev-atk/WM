<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.rsdev.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.WatermarkActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity.IconActivity">
        </activity>
        <activity android:name=".Activity.ImagesInIconActivity">
        </activity>
        <activity android:name=".Activity.CropActivity">
        </activity>
        <activity android:name=".Activity.ModificationImageActivity">
        </activity>
        <activity android:name=".Activity.CombinationActivity">
        </activity>
        <activity android:name=".Activity.IconShowActivity">
        </activity>
    </application>

</manifest>